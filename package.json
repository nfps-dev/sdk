{
	"name": "@nfps.dev/sdk",
	"version": "0.8.0",
	"description": "SDK for NFP development",
	"keywords": [
		"nfp",
		"nfps.dev",
		"secret-network",
		"smart-contract",
		"nft",
		"privacy"
	],
	"homepage": "https://nfps.dev/",
	"repository": "github:nfps.dev/sdk",
	"license": "ISC",
